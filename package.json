    "@tremor/react": "^2.1.0",
    "@vercel/node": "^2.6.2",
    "axios": "^1.3.5",
    "class-variance-authority": "^0.5.2",
    "concurrently": "^8.0.1",
    "isbot": "^3.6.5",
    "prettier": "^2.8.7",
    "prettier-plugin-tailwindcss": "^0.2.7",
    "prisma": "^4.12.0",
    "react": "^18.2.0",
    "react-dom": "^18.2.0"
  },
  "devDependencies": {
    "@remix-run/dev": "^1.15.0",
